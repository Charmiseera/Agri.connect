{
  "version": 2,
  "name": "agri-connect-frontend",
  "env": {
    "NODE_OPTIONS": "--openssl-legacy-provider",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
  },
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
